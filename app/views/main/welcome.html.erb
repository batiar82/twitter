<% if !user_signed_in? %>
<h1>Main#welcome</h1>
<p>Find me in app/views/main/welcome.html.erb</p>
<%else%>
<%= link_to 'New Tweet', new_tweet_path %>
<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
<%= render @tweets %>
 </tbody>
</table>
<%end%>